{ pkgs, lib, config, ... }:
with lib;
let cfg = config.modules.programs.autorandr;
in
{

  options.modules.programs.autorandr = { enable = mkEnableOption "autorandr"; };

  config = mkIf cfg.enable {
    programs.autorandr = {
      enable = true;
      profiles = {
        "single" = {
          fingerprint = {
            DP-2 = "";
          };
          config = {
            DP-2 = {
              enable = true;
              primary = true;
              mode = "1920x1080";
              rate = "144.00";
            };
          };
        };
        "double" = {
          fingerprint = {
            DP-2 = "";
            HDMI-0 = "";
          };
          config = {
            DP-2 = {
              enable = true;
              primary = true;
              mode = "1920x1080";
              rate = "144.00";
              position = "0x500";
            };
            HDMI-0 = {
              enable = true;
              mode = "1920x1080";
              rate = "60.00";
              position = "1920x0";
              rotate = "right";
            };

          };
        };
      };
      hooks.postswitch = {
        "reload eww" = "eww reload";
        "reload bspwm" = "bspc wm -r";
      };



    };


  };
}
